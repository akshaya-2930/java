public class Duplicate {
    public static void main(String[] args) {
        int[] arr = {1, 7, 9, 7, 9, 8, 7, 9};
        System.out.println("Duplicate numbers:");

        for (int i = 0; i < arr.length; i++) {
            for (int j = i + 1; j < arr.length; j++) {
                if (arr[i] == arr[j]) {
                    System.out.println(arr[i]);
                    
                }
            }
        }
    }
}
